{
  "name": "simple-backend-boilerplate",
  "version": "1.0.0",
  "description": "simple backend boilerplate",
  "main": "start.js",
  "private": true,
  "engines": {
    "node": ">= 8"
  },
  "scripts": {
    "start": "node start.js",
    "watch": "nodemon ./start.js --ignore client/",
    "dev": "concurrently \"npm run watch\" \"npm run assets\" --names \"N,W\" --prefix name",
    "assets": "webpack --watch  --mode=development --display-max-modules=0",
    "minify": "webpack --mode=production",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "author": "abhijithvijayan",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.4.3",
    "axios": "^0.18.0",
    "bootstrap": "^4.3.1",
    "dotenv": "^7.0.0",
    "express-handlebars": "^3.0.2",
    "jquery": "^3.3.1",
    "jsonwebtoken": "^8.5.1",
    "keystone": "^4.0.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "popper.js": "^1.14.7"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "autoprefixer": "^9.5.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^1.0.1",
    "concurrently": "^4.1.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.14.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-wesbos": "0.0.19",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.3.0",
    "file-loader": "^3.0.1",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.11",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
