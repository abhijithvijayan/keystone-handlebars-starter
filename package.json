{
  "name": "keystone-handlebars-boilerplate",
  "version": "1.0.0",
  "description": "keystone-handlebars-boilerplate",
  "private": true,
  "author": "abhijithvijayan",
  "repository": "https://github.com/abhijithvijayan/keystone-handlebars-boilerplate.git",
  "engines": {
    "node": ">= 8 <11"
  },
  "workspaces": {
    "packages": [
      "client",
      "server"
    ],
    "nohoist": []
  },
  "scripts": {
    "start": "node start.js",
    "watch": "nodemon ./start.js --ignore client/",
    "dev": "concurrently \"npm run watch\" \"npm run assets\" --names \"N,W\" --prefix name",
    "assets": "webpack --watch  --mode=development --display-max-modules=0",
    "minify": "webpack --mode=production",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "devDependencies": {
    "babel-eslint": "10.0.3",
    "concurrently": "^4.1.2",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-onepass": "1.4.2",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "prettier": "^1.18.2"
  }
}
